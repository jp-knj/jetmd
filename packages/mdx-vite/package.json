{
  "name": "mdx-vite",
  "version": "0.1.0",
  "description": "Vite plugin for MDX",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "biome check src",
    "format": "biome format --write src",
    "release:prepack": "pnpm build",
    "prepublishOnly": "pnpm run release:prepack"
  },
  "dependencies": {
    "mdx-compiler": "workspace:*",
    "faster-md": "workspace:*",
    "@rollup/pluginutils": "^5.0.0",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@biomejs/biome": "^1.5.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jp-knj/jetmd.git",
    "directory": "packages/mdx-vite"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "mdx"
  ],
  "author": "JetMD Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jp-knj/jetmd/issues"
  },
  "homepage": "https://github.com/jp-knj/jetmd"
}
