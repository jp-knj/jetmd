{
  "name": "mdx-compiler",
  "version": "0.1.0",
  "description": "MDX to ESM compiler for faster-md",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "biome check src",
    "format": "biome format --write src"
  },
  "dependencies": {
    "acorn": "^8.11.0",
    "acorn-jsx": "^5.3.2",
    "faster-md": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@biomejs/biome": "^1.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jp-knj/jetmd.git",
    "directory": "packages/mdx-compiler"
  },
  "keywords": [
    "mdx",
    "markdown",
    "jsx",
    "compiler"
  ],
  "author": "JetMD Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jp-knj/jetmd/issues"
  },
  "homepage": "https://github.com/jp-knj/jetmd"
}
