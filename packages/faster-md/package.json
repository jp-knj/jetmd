{
  "name": "faster-md",
  "version": "0.1.0",
  "description": "High-performance Markdown processor with WASM",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "types", 
    "wasm",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "biome check src",
    "format": "biome format --write src",
    "release:prepack": "pnpm build",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm run release:prepack"
  },
  "dependencies": {
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "vitest": "^1.0.0",
    "@biomejs/biome": "^1.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jp-knj/jetmd.git",
    "directory": "packages/faster-md"
  },
  "keywords": [
    "markdown",
    "parser",
    "wasm",
    "commonmark",
    "gfm"
  ],
  "author": "JetMD Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jp-knj/jetmd/issues"
  },
  "homepage": "https://github.com/jp-knj/jetmd"
}
